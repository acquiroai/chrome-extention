{
    "manifest_version": 3,
    "name": "Acquiro AI",
    "description": "Get a personalised Roadmap for your Dream job",
    "version": "0.1",
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "http://127.0.0.1:5000/", "http://127.0.0.1:3000/"
    ],
    "permissions": ["activeTab","tabs","scripting","storage"]
}